{
  "manifest_version": 3,
  "name": "Alt Caption",
  "version": "1.0",
  "description": "Adds buttons to images to generated captions using machine learning",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["<all_urls>"]
    }
    ]
}